# Database (Render PostgreSQL)
DATABASE_URL=postgresql://user:password@host:5432/rt_api

# Cache settings
CACHE_TTL_DAYS=7

# Logging
LOG_LEVEL=INFO

# Authentication
# Generate a secure key: python -c "import secrets; print(secrets.token_hex(32))"
ADMIN_API_KEY=your-secure-admin-key-here

# Default rate limit for regular users (requests per hour)
DEFAULT_RATE_LIMIT=500
